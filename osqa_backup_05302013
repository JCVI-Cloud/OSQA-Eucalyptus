--

CREATE TABLE forum_node (
    id integer NOT NULL,
    title character varying(300) NOT NULL,
    tagnames character varying(125) NOT NULL,
    author_id integer NOT NULL,
    body text NOT NULL,
    node_type character varying(16) NOT NULL,
    parent_id integer,
    abs_parent_id integer,
    added_at timestamp without time zone NOT NULL,
    score integer NOT NULL,
    state_string text NOT NULL,
    last_edited_id integer,
    last_activity_by_id integer,
    last_activity_at timestamp without time zone,
    active_revision_id integer,
    extra text,
    extra_ref_id integer,
    extra_count integer NOT NULL,
    marked boolean NOT NULL
);


